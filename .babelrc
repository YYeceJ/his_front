{
    "presets": [["@babel/preset-env", {"modules": false}], "@babel/preset-react"],
    "plugins": [
        ["@babel/plugin-transform-runtime"],
        ["react-hot-loader/babel"],
        ["@babel/plugin-syntax-dynamic-import"],
        ["import", { libraryName: "antd", style: "css" }] // `style: true` 会加载 less 文件
    ]
}
